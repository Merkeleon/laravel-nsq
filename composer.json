{
  "name": "merkeleon/laravel-nsq",
  "description": "Laravel NSQ",
  "require": {
    "php": "^7.1.3",
    "laravel/framework": "6.*",
    "ext-curl": "*",
    "ext-nsq": "*"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Merkeleon\\Nsq\\NsqServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Merkeleon\\Nsq\\": "src/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
